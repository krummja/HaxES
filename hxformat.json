{
    "lineEnds": {
        "leftCurly": "both",
        "emptyCurly": "noBreak",
        "objectLiteralCurly": {
            "leftCurly": "after"
        }
    },
    "sameLine": {
        "anonFunctionBody": "next",
        "doWhile": "next",
        "tryBody": "keep",
        "tryCatch": "keep"
    },
    "whitespace": {
        "colonPolicy": "after",
        "caseColonPolicy": "after",
        "objectFieldColonPolicy": "after",
        "typeHintColonPolicy": "after",
        "typeCheckColonPolicy": "after",
        "typeParamOpenPolicy": "none",
        "typeParamClosePolicy": "none"
    },
    "emptyLines": {
        "finalNewline": true,
        "lineCommentsBetweenFunctions": "keep"
    },
    "wrapping": {
        "callParameter": {
            "defaultWrap": "keep",
            "rules": [
                {
                    "conditions": [
                        {
                            "cond":"exceedsMaxLineLength",
                            "value": 1
                        }
                    ],
                    "type": "onePerLine"
                }
            ]
        }
    }
}